{
    "name": "vocento/request-id",
    "description": "HTTP Request identifier to trace requests through microservices calls",
    "license": "proprietary",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Vocento\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vocento\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4",
        "ramsey/uuid": "^3|^4"
    },
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    }
}
